{
  "name": "mcp-user-management",
  "version": "1.0.0",
  "description": "Model Context Protocol server for user management",
  "main": "build/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/server.ts",
    "start": "node build/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "server:inspect": "DANGEROUSLY_OMIT_AUTH=true npx @modelcontextprotocol/inspector tsx src/server.ts",
    "clean": "rm -rf build",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "user-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "zod": "^3.25.76",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
